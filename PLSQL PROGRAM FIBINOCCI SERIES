
  1  DECLARE
  2  x number:=0;
  3  y number:=1;
  4  z number;
  5  fibcount number:=2;
  6  n number:=&n;
  7  BEGIN
  8  dbms_output.put_line('Fibinocci Series');
  9  dbms_output.put_line('0');
 10  dbms_output.put_line('1');
 11  while fibcount<n loop
 12  z:=x+y;
 13  x:=y;
 14  y:=z;
 15  dbms_output.put_line(z);
 16  fibcount:=fibcount+1;
 17  end loop;
 18* end;
